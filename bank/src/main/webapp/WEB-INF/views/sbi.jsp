function checkAccBal(account_no,branchCode,userName)
{
	document.getElementById('test1'+account_no).style.textDecoration ="none";
	var redirectUrl="getAccBalcheck.htm";
	var inputData={};
	inputData.account_no=account_no;
	inputData.branchCode=branchCode;
	inputData.userName=userName;
	$.ajax({
		type: "POST",
	     url: redirectUrl,
	     dataType: "text",
	     data: inputData,
	     success:function(response){
	        	var res=JSON.parse(response);
	    		if (res.sessionIsNull == 'YES')
				{
					document.viewAllAccountForm.action = "sessiontimeout.htm";
					document.viewAllAccountForm.submit();
				}
	        	var accval = res.accVal;
	        	 var amount = res.accBalance;
	        	 var accstatus =res.account_status;
	        	 var currencycode = res.currency_code;
	        	 var data = currencycode+"  "+amount;
	        	 if(accval == 'false')
				{
					alert("There is serious irregularities");

				}
	        	 else if(amount!=null && amount!='undefined' && amount!="Please Try Later")
	        	 {
						if(accstatus!=null && (accstatus == 'CLOS' || accstatus == 'DISC'))
						{
							alert("The account seems to be 'Closed' as per bank's record, it will be reflected in closed account list");
						}
						document.getElementById('test1'+account_no).style.display= "none";
						document.getElementById('accBalRes'+account_no).style.display= "";
						document.getElementById('accBalRes'+account_no).innerHTML= data;
				}
				else
				{
						document.getElementById('test1'+account_no).innerHTML= amount;
				}
	     },
			     error:function(response){
			    	 alert("Sorry unable to process the request. Please try later.");
			    
			     }
			});
}

